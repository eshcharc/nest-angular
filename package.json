{
   "name": "nest-typescript-starter",
   "version": "1.0.0",
   "description": "Nest TypeScript starter repository",
   "license": "MIT",
   "scripts": {
      "start": "ts-node src/index.ts",
      "start:watch": "nodemon",
      "prestart:prod": "tsc",
      "start:prod": "ts-node build/src/server.js",
      "migrate:run": "ts-node ./node_modules/.bin/typeorm migrations:run",
      "migrate:revert": "ts-node ./node_modules/.bin/typeorm migrations:revert",
      "migrate:create": "ts-node ./node_modules/.bin/typeorm migrations:create --name",
      "migrate:generate": "ts-node ./node_modules/.bin/typeorm migrations:generate --name",
      "schema:sync": "ts-node ./node_modules/.bin/typeorm schema:sync",
      "build:client-and-server-bundles": "cd client && ng build --prod --build-optimizer && ng build --prod --app 1 --output-hashing=false",
      "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
      "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
      "serve:ssr": "node dist/server"
   },
   "dependencies": {
      "@angular/animations": "^5.1.0",
      "@angular/cdk": "^5.0.1",
      "@angular/common": "^5.0.0",
      "@angular/compiler": "^5.0.0",
      "@angular/core": "^5.0.0",
      "@angular/forms": "^5.0.0",
      "@angular/http": "^5.0.0",
      "@angular/material": "^5.0.1",
      "@angular/platform-browser": "^5.0.0",
      "@angular/platform-browser-dynamic": "^5.0.0",
      "@angular/platform-server": "^5.0.0",
      "@angular/router": "^5.0.0",
      "@nestjs/common": "^4.4.2",
      "@nestjs/core": "^4.4.2",
      "@nestjs/microservices": "^4.4.1",
      "@nestjs/swagger": "^1.1.2",
      "@nestjs/testing": "^4.4.1",
      "@nestjs/websockets": "^4.4.1",
      "@nguniversal/express-engine": "^5.0.0-beta.5",
      "@nguniversal/module-map-ngfactory-loader": "^5.0.0-beta.5",
      "body-parser": "^1.18.2",
      "class-transformer": "^0.1.7",
      "class-validator": "^0.7.2",
      "core-js": "^2.4.1",
      "mysql": "^2.14.1",
      "mysql2": "^1.5.1",
      "redis": "^2.7.1",
      "reflect-metadata": "^0.1.10",
      "rxjs": "^5.5.2",
      "sequelize": "^4.23.1",
      "sequelize-cli": "^3.1.0",
      "sequelize-typescript": "^0.5.0",
      "ts-loader": "^3.2.0",
      "typeorm": "^0.1.6",
      "zone.js": "^0.8.18"
   },
   "devDependencies": {
      "@angular/cli": "^1.6.0",
      "@angular/compiler-cli": "^5.1.0",
      "@types/node": "^8.0.53",
      "@types/sequelize": "^4.0.79",
      "nodemon": "^1.12.1",
      "ts-node": "^3.3.0",
      "typescript": "^2.6.1"
   }
}
